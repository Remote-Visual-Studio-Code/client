{
    "name": "remote-vscode",
    "displayName": "Remote VSCode",
    "description": "Extension for creating and ending new sessions with Remote VSCode.",
    "version": "0.0.1",
    "publisher": "Exortions",
    "engines": {
        "vscode": "^1.66.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:remote-vscode.setPermission",
        "onCommand:remote-vscode.createSession",
        "onCommand:remote-vscode.endSession",
        "onCommand:remote-vscode.kickUser"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "remote-vscode.setPermission",
                "title": "Set Permission"
            },
            {
                "command": "remote-vscode.createSession",
                "title": "Create Session"
            },
            {
                "command": "remote-vscode.endSession",
                "title": "End Session"
            },
            {
                "command": "remote-vscode.kickUser",
                "title": "Kick User"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "yarn run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "yarn run compile && yarn run lint",
        "lint": "eslint",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/express": "^4.17.13",
        "@types/glob": "^7.2.0",
        "@types/mocha": "^9.1.0",
        "@types/node": "14.x",
        "@types/vscode": "^1.66.0",
        "@typescript-eslint/eslint-plugin": "^5.16.0",
        "@typescript-eslint/parser": "^5.16.0",
        "@vscode/test-electron": "^2.1.3",
        "eslint": "^8.11.0",
        "glob": "^7.2.0",
        "mocha": "^9.2.2",
        "prettier": "^2.6.2",
        "typescript": "^4.5.5"
    },
    "license": "MIT",
    "author": "Exortions",
    "dependencies": {
        "axios": "^0.26.1",
        "express": "^4.17.3"
    }
}
